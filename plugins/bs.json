						".*": {
							"commands": [
								{
									"cmd": [
										"tar",
										"xzf",
										"{DL_FILENAME}"
									]
								},
								{
									"flags": {
										"CFLAGS": "-fPIC",
										"LDFLAGS": "-fPIC"
									},
									"cwd": "{DL_DIRECTORY}",
									"cmd": [
										"./configure",
										"--prefix={WORKSPACEDIR}",
										"--static"
									]
								},
								{
									"cwd": "{DL_DIRECTORY}",
									"cmd": [
										"make",
										"-j{NPROC}"
									]
								},
								{
									"cwd": "{DL_DIRECTORY}",
									"cmd": [
										"make",
										"install"
									]
								}
							]
