	"file_definitions": {
		"test-bs-av1.vpy": {
			"path": "{TESTDIR}",
			"encoding": "text/utf-8",
			"data": "import vapoursynth as vs\ncore = vs.core\ncore.std.LoadPlugin('{TESTDIR}/{PLUGIN_FILENAME}')\nc = core.bs.VideoSource('av1-test.webm')\nc.set_output(0)\n"
		},
		"av1-test.webm": {
			"path": "{TESTDIR}",
			"encoding": "base64/zstd",
			"data": ""
		}
	},
	"tests": [
		{
			"name": "test-bs-av1",
			"create_files": [
				"test-bs-av1.vpy",
				"av1-test.webm"
			],
			"commands": [
				{
					"cwd": "{TESTDIR}",
					"cmd": [
						"{VSPIPE}",
						"test-bs-av1.vpy",
						"--"
					]
				}
			]
		}
	],
