{
	"name": "BestSource",
	"type": "VSPlugin",
	"description": "A general source plugin based on FFmpeg with focus on accurate 'seeking'",
	"identifier": "com.vapoursynth.bestsource",
	"github": "https://github.com/vapoursynth/bestsource",
