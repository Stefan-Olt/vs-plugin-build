{
	"name": "FFTSpectrum-rs",
	"type": "VSPlugin",
	"description": "A faster version of the FFT spectrum plugin. Displays the FFT frequency spectrum of a given clip.",
	"identifier": "sgt.fftspectrum_rs",
	"github": "https://github.com/sgt0/vapoursynth-fftspectrum-rs",
	"file_definitions": {
		"test-filter-0.vpy": {
			"path": "{TESTDIR}",
			"encoding": "text/utf-8",
			"data": "import vapoursynth as vs\ncore = vs.core\ncore.std.LoadPlugin('{TESTDIR}/{PLUGIN_FILENAME}')\nc = core.std.BlankClip(format=vs.YUV422PS,width=320,height=240,length=10)\nc = core.fftspectrum_rs.FFTSpectrum(c)\nc.set_output(0)\n"
		}
	},
	"tests": [
		{
			"name": "test-filter-0",
			"create_files": [
				"test-filter-0.vpy"
			],
			"commands": [
				{
					"cwd": "{TESTDIR}",
					"cmd": [
						"{VSPIPE}",
						"test-filter-0.vpy",
						"--"
					]
				}
			]
		}
	],
	"releases": [
		{
			"version": "v1.0.10",
			"published": "2025-10-22T20:06:12Z",
			"source": "https://github.com/sgt0/vapoursynth-fftspectrum-rs/archive/refs/tags/v1.0.10.tar.gz",
			"hash": "73a7965fb039e38a75f1e724969241941c5b6927014fa1c5d509155200622b22",
			"filename": "vapoursynth-fftspectrum-rs-1.0.10.tar.gz",
			"additional_files": {
				".*": [
					"{BUILDDIR}/{DL_DIRECTORY}/LICENSE.md",
					"{BUILDDIR}/{DL_DIRECTORY}/README.md"
				]
			},
			"build": {
				".*": {
					"commands": [
						{
							"cmd": [
								"tar",
								"xzf",
								"{DL_FILENAME}"
							]
						},
						{
							"cwd": "{DL_DIRECTORY}",
							"cmd": [
								"cargo",
								"build",
								"--release",
								"--target",
								"{RUST_TARGET}"
							]
						},
						{
							"cwd": "{WORKSPACEDIR}",
							"cmd": [
								"mkdir",
								"-p",
								"lib"
							]
						},
						{
							"cwd": "{DL_DIRECTORY}/target/{RUST_TARGET}/release",
							"cmd": [
								"cp",
								"libfftspectrum_rs.{LIB_EXTENSION}",
								"{WORKSPACEDIR}/lib/"
							]
						}
					],
					"dependencies": []
				}
			},
			"buildtools_dependencies": {
				".*": [
					{
						"name": "rustc",
						"version": [
							">=",
							"1.92.0"
						]
					}
				]
			},
			"os-min-version": {
				"darwin-x86_64": "10.12"
			},
			"release_files": {
				"linux-.*": [
					"{WORKSPACEDIR}/lib/libfftspectrum_rs.so"
				],
				"darwin-.*": [
					"{WORKSPACEDIR}/lib/libfftspectrum_rs.dylib"
				]
			},
			"tests": {
				".*": [
					"test-filter-0"
				]
			}
		}
	]
}