			"filename": "VapourSynth-EEDI2-r7.1.tar.gz",
			"additional_files": {
				".*": [
					"{BUILDDIR}/{DL_DIRECTORY}/LICENSE",
					"{BUILDDIR}/{DL_DIRECTORY}/README.md"
				]
			},
			"build": {
				".*": {
					"commands": [
						{
							"cmd": [
								"tar",
								"xzf",
								"{DL_FILENAME}"
							]
						},
						{
							"cwd": "{DL_DIRECTORY}",
							"cmd": [
								"meson",
								"rewrite",
								"kwargs",
								"delete",
								"target",
								"eedi2",
								"install_dir",
								"foobar"
							]
						},
						{
							"cwd": "{DL_DIRECTORY}",
							"cmd": [
								"meson",
								"setup",
								"--prefix={WORKSPACEDIR}",
								"--libdir={WORKSPACEDIR}/lib",
								"build"
							]
						},
						{
							"cwd": "{DL_DIRECTORY}",
							"cmd": [
								"ninja",
								"-C",
								"build"
							]
						},
						{
							"cwd": "{DL_DIRECTORY}",
							"cmd": [
								"ninja",
								"-C",
								"build",
								"install"
							]
						}
					],
					"dependencies": []
				}
			},
			"buildtools_dependencies": {
				".*": [
					{
						"name": "meson",
						"version": [
							">=",
							"0.60.0"
						]
					},
					{
						"name": "ninja",
						"version": [
							">=",
							"1.10.0"
						]
					}
				]
			},
			"release_files": {
				"linux-.*": [
					"{WORKSPACEDIR}/lib/libeedi2.so"
				],
				"darwin-.*": [
					"{WORKSPACEDIR}/lib/libeedi2.dylib"
				]
			},
			"tests": {
				".*": [
					"test-filter-0"
				]
			}
		}
	]
